    "404": {
      "redirect": false,
      "title": "Page Not Found",
      "description": "Well… that didn’t work. Don’t worry, probability of success was low anyway."
    },
    "500": {
      "title": "Server Meltdown",
      "description": "Something broke. Probably not your fault… probably."
    },
    "503": {
      "title": "Service Unavailable",
      "description": "The server is taking a break. Try again when it feels like working."
    },
    "504": {
      "title": "Timeout",
      "description": "The server waited. And waited. And finally gave up. Understandable."
    }
  }
